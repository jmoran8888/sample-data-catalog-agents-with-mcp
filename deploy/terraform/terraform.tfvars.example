# Copy this file to terraform.tfvars and fill in your values

# Database password (optional - has default)
# db_password = "YourSecurePassword123!"

# AWS region (optional - defaults to us-east-1)
# aws_region = "us-east-1"

# Environment name (optional - defaults to dev)
# environment = "dev"

# IP Address Whitelist (REQUIRED for security)
# Get your IP with: curl ifconfig.me
allowed_ip_address = ""  # Set to your IP address (e.g., "1.2.3.4")
